-- CAU CHUA LAM : 18, 35, 37, 38, 39, 40, 41, 42, 43, 44
-- DANG UPDATE

1.	SELECT MASP, TENSP 
	FROM SANPHAM
	WHERE NUOCSX = 'Trung Quoc'

2.	SELECT MASP, TENSP 
	FROM SANPHAM
	WHERE DVT = 'cay' OR DVT = 'quyen'

3.	SELECT MASP, TENSP 
	FROM SANPHAM
	WHERE LEFT(MASP,1) = 'B' AND RIGHT(MASP, 2) = '01'

4.	SELECT MASP, TENSP 
	FROM SANPHAM	
	WHERE  GIA BETWEEN 30000 AND  40000

5.	SELECT MASP, TENSP 
	FROM SANPHAM
	WHERE (NUOCSX = 'Trung Quoc' OR NUOCSX = Thai Lan')
	AND (GIA BETWEEN 30000 AND 40000)

6.	SELECT SOHD, TRIGIA 
	FROM HOADON
	WHERE NGHD = '1/1/2007' OR NGHD = '2/1/2007'

7.	SELECT SOHD, TRIGIA FROM HOADON
	WHERE MONTH(NGHD) = '1' AND YEAR(NGHD) = '2007'
	ORDER BY DAY(NGHD) , TRIGIA DESC


8. 	SELECT HD.MAKH, HOTEN 
 	FROM HOADON HD JOIN KHACHHANG KH 
		ON HD.MAKH = HOTEN.MAKH
      	 WHERE NGHD = '1/1/2007'

9.	SELECT SOHD, TRIGIA 
	FROM HOADON JOIN NHANVIEN 
		ON HOADON.MANV = NHANVIEN.MANV
	WHERE  HOTEN = 'Nguyen Van B' AND NGHD = '28/10/2006'

10.	SELECT SOHD, TRIGIA 
	FROM HOADON JOIN KHACHHANG
		ON HOADON.MAKH = KHACHHANG.MAKH
	WHERE  HOTEN = 'Nguyen Van A' AND MONTH(NGHD) = '10' AND YEAR(NGHD) = '2006'

11. 	SELECT SOHD
	FROM CTHD
	WHERE MASP = 'BB01' OR MASP = 'BB02'

12.	SELECT SOHD
	FROM CTHD
	WHERE (MASP = 'BB01' OR MASP = 'BB02') AND (10 <= SL AND SL <= 20)

13.	SELECT SOHD FROM CTHD
	WHERE MASP = 'BB01' AND (10 <= SL AND SL <= 20)
	INTERSECT
	SELECT SOHD FROM CTHD
	WHERE MASP = 'BB02' AND (10 <= SL AND SL <= 20)

14.	SELECT SANPHAM.MASP, TENSP
	FROM SANPHAM
	JOIN CTHD
		ON SANPHAM.MASP = CTHD.MASP
	JOIN HOADON
		ON CTHD.SOHD = HOADON.SOHD
	WHERE NGHD = '1/1/2007' OR NUOCSX = 'Trung Quoc'
	

15.	SELECT MASP, TENSP FROM SANPHAM
	WHERE MASP NOT IN (SELECT MASP FROM CTHD)

16.	SELECT MASP, TENSP FROM SANPHAM
	WHERE MASP NOT IN (SELECT MASP
			FROM CTHD JOIN HOADON ON CTHD.SOHD = HOADON.SOHD
			WHERE YEAR(NGHD) = '2006')

17.	SELECT MASP, TENSP FROM SANPHAM
	WHERE NUOCSX = 'Trung Quoc' 
	AND MASP NOT IN (SELECT MASP
			FROM CTHD JOIN HOADON ON CTHD.SOHD = HOADON.SOHD
			WHERE NUOCSX = 'Trung Quoc' AND YEAR(NGHD) = '2016')

18.	SELECT SOHD FROM CTHD
	WHERE MASP = 'BB01' AND (10 <= SL AND SL <= 20)

19.	SELECT COUNT(SOHD) 
	FROM HOADON
	WHERE MAKH = NULL

20.	SELECT COUNT(MASP) FROM SANPHAM
	WHERE MASP IN (SELECT MASP FROM CTHD)

21.	SELECT MAX(TRIGA) GIACAONHAT, MIN(TRIGIA) GIATHAPNHAT
	FROM HOADON


22.	SELECT AVG(TRIGA) GIATB
	FROM HOADON
	WHERE YEAR(NGHD) = '2006'


23.	SELECT SUM(TRIGA) DOANHTHU
	FROM HOADON
	WHERE YEAR(NGHD) = '2006'


24.	SELECT SOHD 
	FROM HOADON 
	WHERE YEAR(NGHD) = '2006'
	GROUP BY SOHD, TRIGIA, NGHD
	HAVING TRIGIA >= ALL ( SELECT TRIGIA FROM HOADON
				WHERE YEAR(NGHD) = '2006' )


25.	SELECT HOTEN
	FROM HOADON JOIN KHACHHANG
		ON HOADON.MAKH = CTHD.MAKH
	WHERE YEAR(NGHD) = '2006'
	GROUP BY HOTEN, TRIGIA, NGHD
	HAVING TRIGIA >= ALL ( SELECT TRIGIA FROM HOADON


26.	SELECT TOP 3 MAKH, HOTEN FROM KHACHHANG
	ORDER BY DOANHSO DESC


27.	SELECT MASP, TENSP FROM SANPHAM
	WHERE GIA IN (	SELECT TOP 3 GIA FROM SANPHAM
	ORDER BY DIEM DESC)


28.	SELECT MASP, TENSP FROM SANPHAM
	WHERE NUOCSX = 'Thai Lan' AND
	GIA IN (SELECT TOP 3 GIA FROM SANPHAM
		ORDER BY DIEM DESC)



29.	SELECT MASP, TENSP FROM SANPHAM
	WHERE NUOCSX = 'Trung Quoc' AND
	GIA IN (SELECT TOP 3 GIA FROM SANPHAM
		WHERE NUOCSX = 'Trung Quoc'
		ORDER BY DIEM DESC)


30.	SELECT TOP 3 * FROM KHACHHANG
	ORDER BY DOANHSO DESC


31.	SELECT COUNT(MASP) TONG SP 
	FROM SANPHAM
	WHERE NUOCSX = 'Trung Quoc'


32.	SELECT NUOCSX, COUNT(MASP) TONGSP 
	FROM SANPHAM
	GROUP BY NUOCSX


33.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX


34.	SELECT NGHD, SUM(TRIGIA) TONGDTNGAY 
	FROM HOADON 
	GROUP BY NGHD

35.	SELECT SANPHAM.MASP, SUM(SL) TONGSL
	FROM SANPHAM JOIN CTHD
		ON SANPHAM.MASP = CTHD.MASP
	WHERE 
	GROUP BY MASP


36.	SELECT MONTH(NGHD),YEAR(NGHD), SUM(TRIGIA) TONGDTTHANG 
	FROM HOADON 
	WHERE YEAR(NGHD) = '2006'
	GROUP BY MONTH(NGHD), YEAR(NGHD)

37.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX


38.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX



39.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX



40.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX



41.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX


42.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX



43.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX



44.	SELECT NUOCSX, MAX(MASP) CAONHAT, MIN(MASP) THAP NHAT 
	FROM SANPHAM
	GROUP BY NUOCSX
